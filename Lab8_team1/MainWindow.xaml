<Window x:Class="Lab8_team1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab8_team1"
        mc:Ignorable="d"
        Title="Application" Height="150" Width="400"
        WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Name="lbBiosVersion" FontSize="15" VerticalAlignment="Center"/>
        <Label Grid.Row="1" Name="lbBiosVendor" FontSize="15" VerticalAlignment="Center"/>
        <Label Grid.Row="2" Name="lbBiosCpuType" FontSize="15" VerticalAlignment="Center"/>
    </Grid>
</Window>
